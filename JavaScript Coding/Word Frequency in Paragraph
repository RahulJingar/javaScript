//40. Word Frequency in Paragraph

let paragraph = "Hello world hello Hello";
let words = paragraph.split(' '); 
let freq = {};

for (let i = 0; i < words.length; i++) {
    let word = words[i];
    if (freq[word]) {
        freq[word] += 1;
    } else {
        freq[word] = 1;
    }
}

console.log(freq); 
